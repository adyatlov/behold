{
  "name": "@dyatlov.net/behold",
  "version": "1.0.1",
  "description": "Kubernetes object viewer",
  "author": "Andrey Dyatlov @adyatlov",
  "bin": {
    "behold": "./bin/run"
  },
  "homepage": "https://github.com/adyatlov/behold",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "adyatlov/behold",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@oclif/core": "^1.16.5",
    "csv": "^6.2.0",
    "csv-writer": "^1.6.0",
    "klaw": "^4.0.1",
    "klaw-sync": "^6.0.0",
    "path-parse": "^1.0.7"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.2",
    "@types/chai": "^4",
    "@types/klaw": "^3.0.3",
    "@types/klaw-sync": "^6.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.64",
    "@types/path-parse": "^1.0.19",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "chai": "^4",
    "eslint": "^8.25.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "behold",
    "dirname": "behold",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "prisma": "^4.4.0",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/adyatlov/behold/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
